escape ^Tt
termcap kterm hs@
terminfo kterm hs@
vbell on
bind w windowlist -b
hardstatus alwayslastline "[%Y/%m/%d %02c:%s] [load %l] %`%-w%
{=b bw}
%n %t%{-}%+w"
#hardstatus alwayslastline I%wI
defhstatus I^En:^EtI
shell -$SHELL
startup_message off
autodetach on
bind -c REGION 2 split
bind -c REGION 1 only
bind -c REGION 0 remove
bind -c REGION o focus
bind -c REGION ^ resize
bind x command -c REGION
bind ^U encoding utf8
bind ^E encoding euc
bind ^S encoding sjis
defscrollback 100000
